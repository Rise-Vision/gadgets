<?xml version="1.0" encoding="UTF-8"?>
<Template>
    <Style>
	<![CDATA[
	* {
	    margin: 0;
	    padding: 0;
	    -moz-box-sizing: border-box;
	    box-sizing: border-box;
	}
	body {
	    background-color: transparent;
	    -webkit-user-select: none;
	    -khtml-user-select: none;
	    -moz-user-select: none;
	    -o-user-select: none;
	    user-select: none;
	}
	#scrollContainer {
	    height: 100%;
	    overflow: hidden;
	}
	ul {
	    list-style-type: none;
	}
	.listing {
	    position: relative;    
	}
	.image {
	    background-size: contain;
	    float: left;
	    width: 25%;
	    margin-left: 0.5em;
	}
	.content {
	    margin-left: 25%;
	    padding: 0 0.5em 0 1em;
	}	
	p {
	    margin: 0 0 0.5em 0;
	}
	hr {
	    clear: both;
	}
	.noData {
	    line-height: 2em;
	    text-align: center;
	}
	]]>
    </Style>
    <Layout>
	<![CDATA[
	<ul id="directory" class="page">
	    <li class="listing item">
		<!-- If Image Column is specified in the Gadget settings, the image will be inserted here automatically. -->
		<div class="content">
		    <p class="A heading_font-style"></p>		    
		    <span class="C data_font-style"></span><br />
		    <span class="D data_font-style"></span><br />
		    <span class="E data_font-style"></span><br />
		    <span class="F data_font-style"></span><br />
		</div>
		<hr />
	    </li>
	</ul>
	]]>
    </Layout>
</Template>

